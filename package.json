{
  "name": "test-login-screen",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.28",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts-ts": "^2.6.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run server:build-and-start",
    "server:prod": "cross-env NODE_ENV=production npm run server:build-and-start",
    "server:build-and-start": "npm run build && ts-node server.ts",
    "test": "react-scripts-ts test --env=jsdom --coverage",
    "tdd": "react-scripts-ts test --env=jsdom --watchAll",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/react-redux": "^5.0.8",
    "@types/redux-form": "^7.0.5",
    "codecov": "^2.3.0",
    "cross-env": "^5.0.5",
    "express": "^4.15.4",
    "jest-cli": "^21.0.2",
    "node-sass-chokidar": "0.0.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-observable": "^0.16.0",
    "reselect": "^3.0.1",
    "ts-node": "^3.3.0"
  }
}
